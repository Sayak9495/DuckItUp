{

  "manifest_version": 2,
  "name": "DuckItUp",
  "description": "DuckDuckGo Extension",
  "version": "0.1",
  "icons": {"128": "icon_128.png"},
  "browser_action":{
    "default_icon": "icon.png"
  },
    "content_scripts": [
      {
        "matches": [
          "https://duckduckgo.com/*"
        ],
        "js": ["script.js"],
        "run_at": "document_idle"
      }
    ],
    "permissions": ["activeTab"]
}